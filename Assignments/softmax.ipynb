    "import numpy as np\n",
    "\n",
    "def softmax(x):\n",
    "    return np.exp(x) / sum(np.exp(x))\n",
    "\n",
    "x = np.array([int(x) for x in input().split()])\n",
    "\n",
    "print(softmax(x))"
   ]
